<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.peyton.plum.mall.mapper.DictMapper">
	<resultMap id="ResultDictMap" type="cn.peyton.plum.mall.pojo.Dict">
		<id column="id" property="id" jdbcType="BIGINT"/>
		<result column="name" property="name" jdbcType="VARCHAR"/>
		<result column="explain" property="explain" jdbcType="VARCHAR"/>
	</resultMap>

	<sql id="Column_sys_dict_List">
		`id`,`name`,`explain`
	</sql>

	<insert id="insertSelective" parameterType="cn.peyton.plum.mall.pojo.Dict" keyColumn="id" keyProperty="id" useGeneratedKeys="true">
		insert into `sys_dict`
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="name != null">`name`,</if>
			<if test="explain != null">`explain`,</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="name != null">#{name},</if>
			<if test="explain != null">#{explain},</if>
		</trim>
	</insert>

	<delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
		delete from `sys_dict`
		where `id` = #{id}
	</delete>

	<update id="updateByPrimaryKeySelective" parameterType="cn.peyton.plum.mall.pojo.Dict">
		update `sys_dict`
		<set>
			<if test="name != null">`name` = #{name},</if>
			<if test="explain != null">`explain` = #{explain},</if>
		</set>
		where id = #{id}
	</update>

	<select id="selectByPrimaryKey" resultMap="ResultDictMap"  parameterType="java.lang.Long">
		select
		<include refid="Column_sys_dict_List"/>
		from `sys_dict`
		where `id` = #{id}
	</select>

	<!-- new create method   -->	

	<select id="selectByAllOrKeyword" resultMap="ResultDictMap"  parameterType="map">

	</select>

	<select id="selectByObj" resultMap="ResultDictMap"  parameterType="map">

	</select>

	<select id="count" resultType = "java.lang.Integer" parameterType="java.lang.String">

	</select>

	<select id="isRename" resultType = "java.lang.Integer" parameterType="cn.peyton.plum.mall.pojo.Dict">

	</select>

	<update id="upStatus" parameterType="cn.peyton.plum.mall.pojo.Dict">

	</update>

</mapper>
