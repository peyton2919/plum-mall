	</delete>

	<update id="updateByPrimaryKeySelective" parameterType="cn.peyton.plum.mall.pojo.SupplierLog">
		update `sys_supplier_log`
		<set>
			<if test="supplierId != null">`supplier_id` = #{supplierId},</if>
			<if test="oldValue != null">`old_value` = #{oldValue},</if>
			<if test="newValue != null">`new_value` = #{newValue},</if>
			<if test="operateIp != null">`operate_ip` = #{operateIp},</if>
			<if test="createTime != null">`create_time` = #{createTime},</if>
			<if test="status != null">`status` = #{status},</if>
		</set>
		where id = #{id}
	</update>

	<select id="selectByPrimaryKey" resultMap="ResultSupplierLogMap"  parameterType="java.lang.Long">
		select
		<include refid="Column_sys_supplier_log_List"/>
