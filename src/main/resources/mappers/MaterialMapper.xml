		where `id` = #{id}
	</delete>

	<update id="updateByPrimaryKeySelective" parameterType="cn.peyton.plum.mall.pojo.Material">
		update `sys_material`
		<set>
			<if test="createId != null">`create_id` = #{createId},</if>
			<if test="type != null">`type` = #{type},</if>
			<if test="groupId != null">`group_id` = #{groupId},</if>
			<if test="name != null">`name` = #{name},</if>
			<if test="url != null">`url` = #{url},</if>
			<if test="createType != null">`create_type` = #{createType},</if>
			<if test="createTime != null">`create_time` = #{createTime},</if>
		</set>
		where id = #{id}
	</update>

	<select id="selectByPrimaryKey" resultMap="ResultMaterialMap"  parameterType="java.lang.Long">
		select
		<include refid="Column_sys_material_List"/>
		from `sys_material`
		where `id` = #{id}
